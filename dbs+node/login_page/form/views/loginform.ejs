<table border="1" cellpading="7" cellspacing="7">
<tr>
<th>Id</th>
<th>Name</th>
<th colspan=2>Action</th>
</tr>
<%if(data.length){
for(var i=0;i<data.length;i++){%>
<tr>
<td><%=data[i].id%></td>
<td><%=data[i].name%></td>
<td><a href="/delete/<%=data[i].id%>">Delete</a></td>
<td><a href="/edit<%=data[i].id%>">Edit</a></td>
</tr>
<p align="center"><a href="/edit">Send</a></p>
<%}
}else{%>
<tr>
<td colspan="3">NO User Found</td>
</tr>
<%}%>
</table>

