<table boredr="1" cellpadding="7" cellspacing="7">
<tr>
	<th>no</th>
	<th>Username</th>
	<th>Password</th>
	<th>Delete</th>
	<th>Edit</th>
</tr>

<%if(data.length){
	for(var i=0; i<data.length; i++){%>
	
	<tr>
		<td><%=(i+1)%></td>
		<td><%=data[i].username%></td>
		<td><%=data[i].password%></td>
		<td><a href="/delete/<%=data[i].id%>">Delete</a></td>
		<td><a href="/edit/<%=data[i].id%>">Edit</a></td>
	</tr>
	<%}
}
else
{%>
<tr>
	<td colspan="3">No user found</td>
</tr>
<%}%>
</table>
